[user]
  name = Yuichiro NAKAGAWA
  email = ii.hsif.drows@gmail.com
[url "git@github.com:Mekajiki/"]
  insteadof = gh:
[url "grg@mekajiki.net:/home/grg/repos/"]
  insteadof = mek:
[url "ssh://git@bitbucket.org/lambdainc/"]
  insteadof = lambda:
[github]
  user = mekajiki
[core]
  editor = /usr/bin/vim
  excludesfile = ~/.gitignore_global
  whitespace = trailing-space,space-before-tab
[apply]
  whitespace = fix 
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = auto
[rebase]
  autosquash = true
[push]
  default = simple
[diff]
  compactionHeuristic = true
[alias]
	pushf = push --force-with-lease
[hub]
	protocol = ssh
